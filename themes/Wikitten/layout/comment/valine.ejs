<% if (typeof(script) !== 'undefined' && script) { %>
    <script type="text/javascript" src="http://v2.uyan.cc/code/uyan.js?uid=<%= theme.comment.youyan %>"></script>
<% } else { %>
    <div id="valine-comments"></div>
    <script src="//cdn1.lncld.net/static/js/3.0.4/av-min.js"></script>
    <script src='//unpkg.com/valine/dist/Valine.min.js'></script>
    <script>
        new Valine({
            el: '#valine-comments',
            app_id: "<%= theme.comment.valine.leancloud_appid %>",
            app_key: "<%= theme.comment.valine.leancloud_appkey %>",
            placeholder: "<%= theme.comment.valine.placeholder %>" ,
        })
    </script>
<% } %>